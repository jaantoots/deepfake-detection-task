[MASTER]

extension-pkg-whitelist=numpy,
                        torch

[MESSAGES CONTROL]

disable=bad-continuation,  # handled by black
        ungrouped-imports,  # handled by isort
        duplicate-code  # I know this is bad


[REPORTS]

output-format=colorized


[FORMAT]

expected-line-ending-format=LF

max-line-length=88


[TYPECHECK]

generated-members=torch.*
